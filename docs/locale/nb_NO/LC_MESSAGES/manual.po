msgstr ""

#: ../../manual/automapping.rst:559
msgid "output2\\_Ground"
msgstr ""

#: ../../manual/automapping.rst:561
msgid "output3\\_Ground"
msgstr ""

#: ../../manual/automapping.rst:563
msgid "output4\\_Ground"
msgstr ""

#: ../../manual/automapping.rst:565
